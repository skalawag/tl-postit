<%= render "shared/content_title", title: "Sign up for Postit here." %>

<%= render partial: "shared/error", object: @user %>

<%= form_for @user do |f| %>
  <%= f.label :username %>
  <%= f.text_field :username %>
  <%= f.label :password %>
  <%= f.text_field :password %>
  <br/>
  <%= f.submit class: "btn btn-primary" %>
<% end %>
